{
  "compileOnSave": false,
   "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "@shared/*": ["src/app/modules/user/services/shared.service"]
  },
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "moduleResolution": "node",
    "importHelpers": true,
    "target": "es2015",
    "module": "es2020",
    "lib": [
      "es2018",
      "dom"
    ]
  }
}
